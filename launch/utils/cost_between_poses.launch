<launch>
    <!-- Arguments for remapping topics -->
    <arg name="experiment_bag_path" default="/home/said/fxpbags/rvr1/staticrvr1_synced_transformed.bag" />
    <arg name="ground_truth_bag_path" default="/home/said/fxpbags/tracking/trackstaticrvr1_synced_ground_truth.bag" />

    <!-- Node to calculate cost between poses -->
  <node pkg="mercator_sensor_fusion_ros_pkg" type="cost_between_poses" name="cost_between_poses" output="screen">
    <param name="experiment_bag_path" type="string" value="$(arg experiment_bag_path)" />
    <param name="ground_truth_bag_path" type="string" value="$(arg ground_truth_bag_path)" />
    </node>
</launch>
